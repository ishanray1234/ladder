//1
#include <stdio.h>

int main()
{
    int n,m;
    int x=-1,y=-1,z=100,w=100;
    scanf("%d%d",&n,&m);
    
    char num[n][m];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            scanf("%c",&num[i][j]);
            if(num[i][j]=='\n')
            {
                j--;
            }
        }
    }
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            if(num[i][j]=='*')
            {
                if(i<z)
                    z=i;
                if(i>x)
                    x=i;
                if(j<w)
                    w=j;
                if(j>y)
                    y=j;
            }
        }
    }
    
    for(int i=z;i<=x;i++)
    {
        for(int j=w;j<=y;j++)
        {
            printf("%c",num[i][j]);
        }
        printf("\n");
    }
    return 0;
}

//3
#include <stdio.h>
#include<string.h>

int main()
{
    char str[100];
    scanf("%s",str);
    
    int x=0,y=0;
    
    int s=strlen(str);
    for(int i=0;i<s;i++)
    {
        if(str[i]>='a' && str[i]<='z')
            x++;
        else
            y++;
    }
    if(x>=y)
    {
        for(int k=0;k<=s;k++)
        {
            if(str[k]>='A' && str[k]<='Z')
            {
                str[k]=str[k]-'A'+'a';
            }
        }
    }
    else
    {
        for(int k=0;k<=s;k++)
        {
            if(str[k]>='a' && str[k]<='z')
            {
                str[k]=str[k]-'a'+'A';
            }
        }
    }
    
    printf("%s",str);
    
    return 0;
}
//4
#include <stdio.h>
#include<string.h>

int main()
{
    int n;
    scanf("%d",&n);
    char arr[200];
    
    for(int i=0;i<n;i++)
    {
        scanf("%s",arr);
        int s=strlen(arr);
        
        if(s>10)
            printf("%c%d%c",arr[0],s-2,arr[s-1]);
        else
            printf("%s",arr);
        
        printf("\n");
    }
    
    return 0;
}

